<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!-- Include domain tables -->
    <include file="db.changelog-create-tables.xml" relativeToChangelogFile="true"/>

    <!-- Include initial data -->
    <include file="db.changelog-initial-data.xml" relativeToChangelogFile="true"/>

    <!-- Include ACL tables and data -->
    <include file="db.changelog-acl.xml" relativeToChangelogFile="true"/>

</databaseChangeLog>

